  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --watch",
    "test": "NODE_ENV=test jest --config=jest.config.js",
    "test:watch": "NODE_ENV=test jest --config=jest.config.js --watch",
    "test:coverage": "NODE_ENV=test jest --config=jest.config.js --coverage",
    "lint": "npm-run-all eslint jshint prettier stylelint size",
    "lint:fix": "npm-run-all prettier:fix eslint:fix stylelint:fix",
    "eslint": "eslint --ext .json,.html,.js,.vue --ignore-path=.gitignore src",
    "eslint:fix": "eslint --ext .json,.html,.js,.vue --ignore-path=.gitignore --fix src",
    "jshint": "jshint --extract=auto -e .html,.js,.vue src",
    "prettier": "prettier -c 'src/**/*.@(js|vue)'",
    "prettier:fix": "prettier --write 'src/**/*.@(js|vue)'",
    "stylelint": "stylelint 'src/**/*.@(css|postcss|html)'",
    "stylelint:fix": "stylelint 'src/**/*.@(css|postcss|html)' --fix",
    "size": "size-limit",
    "size:why": "npm run size -- --why",
    "build": "NODE_ENV=production webpack --env.API_URL=${API_URL}",
    "build:dev": "NODE_ENV=development webpack"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "engines": {
    "npm": "6.10.0",
    "node": "11.14.0"
  },
  "browserslist": [
    "last 2 major version"
  ],
  "size-limit": [
    {
      "limit": "1 s",
      "path": "dist/*"
    }
  ],
